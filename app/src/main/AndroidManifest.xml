<manifest xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <application>

        <activity android:name=".activities.SplashActivity"

            android:exported="true"
            android:label="MediAssist"
            android:roundIcon="@mipmap/ic_logo1"
            android:supportsRtl="true"
            android:theme="@style/Theme.MediAssist">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".activities.onboarding.OnboardingActivity"
            android:exported="true"
            android:theme="@style/Theme.MediAssist.NoActionBar">

        </activity>

        <activity
            android:name=".activities.WelcomeActivity"
            android:exported="true"
            android:theme="@style/Theme.MediAssist.NoActionBar" />
        <activity android:name=".activities.Auth.SignUpActivity"
            android:exported="true"
            android:theme="@style/Theme.MediAssist.NoActionBar" />
    </application>




</manifest>
